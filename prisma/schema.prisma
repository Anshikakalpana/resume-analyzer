generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Resume {
  id             Int      @id @default(autoincrement())
  companyName    String
  jobTitle       String
  jobDescription String
  fileName       String
  createdAt      DateTime @default(now())
}
